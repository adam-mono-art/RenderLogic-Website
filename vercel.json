{
  "rewrites": [
    { "source": "/portfolio/paddle", "destination": "https://paddle-visualizer.vercel.app/" },
    { "source": "/portfolio/paddle/(.*)", "destination": "https://paddle-visualizer.vercel.app/$1" },
    { "source": "/assets/(.*)", "destination": "https://paddle-visualizer.vercel.app/assets/$1"},
    { "source": "/models/(.*)", "destination": "https://paddle-visualizer.vercel.app/models/$1"},
    { "source": "/textures/(.*)", "destination": "https://paddle-visualizer.vercel.app/textures/$1"},
    { "source": "/images/(.*)", "destination": "https://paddle-visualizer.vercel.app/images/$1"},
    { "source": "/portfolio/watch", "destination": "https://watch-visualizer.vercel.app/" },
    { "source": "/portfolio/watch/(.*)", "destination": "https://watch-visualizer.vercel.app/$1" },
    { "source": "/portfolio/patio3d", "destination": "https://frame-visualizer.vercel.app/" },
    { "source": "/portfolio/patio3d/(.*)", "destination": "https://frame-visualizer.vercel.app/$1" }
  ]
}